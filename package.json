{
  "name": "electron-vue-starter",
  "version": "0.1.0",
  "description": "Electron-Vue Starter.",
  "main": "dist/main.js",
  "scripts": {
    "predev": "npm run compile",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run compile:main -- --watch\" \"npm run compile:renderer -- --watch\" \"electron .\"",
    "compile": "npm run compile:main && npm run compile:renderer",
    "compile:main": "babel src/main -d dist",
    "compile:renderer": "webpack --config task/webpack.config.js --hide-modules",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "jest test/feature",
    "test:unit": "jest test/unit"
  },
  "author": "archco",
  "license": "MIT",
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.13",
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "electron": "^1.8.4",
    "electron-builder": "^20.8.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "jest": "^22.4.3",
    "node-sass": "^4.8.3",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "postcss-loader": "^2.1.3",
    "sass-loader": "^6.0.7",
    "source-map-loader": "^0.2.3",
    "spectron": "^3.8.0",
    "style-loader": "^0.20.3",
    "vue": "^2.5.16",
    "vue-jest": "^2.3.0",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.3.0",
    "webpack-cli": "^2.0.13"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": "chrome >= 59"
          }
        }
      ]
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/renderer/script/$1"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"
    }
  }
}
